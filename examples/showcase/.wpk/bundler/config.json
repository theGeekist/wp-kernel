{
	"driver": "rollup",
	"input": {
		"index": "src/index.ts"
	},
	"outputDir": "build",
	"format": "esm",
	"external": [
		"@loglayer/shared",
		"@loglayer/transport",
		"@loglayer/transport-simple-pretty-terminal",
		"@wordpress/api-fetch",
		"@wordpress/components",
		"@wordpress/data",
		"@wordpress/dataviews",
		"@wordpress/element",
		"@wordpress/hooks",
		"@wordpress/i18n",
		"@wordpress/interactivity",
		"@wpkernel/core",
		"@wpkernel/ui",
		"loglayer",
		"react",
		"react-dom",
		"react/jsx-dev-runtime",
		"react/jsx-runtime"
	],
	"globals": {
		"@wordpress/api-fetch": "wp.apiFetch",
		"@wordpress/components": "wp.components",
		"@wordpress/data": "wp.data",
		"@wordpress/dataviews": "wp.dataviews",
		"@wordpress/element": "wp.element",
		"@wordpress/hooks": "wp.hooks",
		"@wordpress/i18n": "wp.i18n",
		"@wordpress/interactivity": "wp.interactivity",
		"react": "React",
		"react-dom": "ReactDOM",
		"react/jsx-dev-runtime": "React",
		"react/jsx-runtime": "React"
	},
	"alias": [
		{
			"find": "@/",
			"replacement": "./src/"
		}
	],
	"sourcemap": {
		"development": true,
		"production": false
	},
	"optimizeDeps": {
		"exclude": [
			"@loglayer/shared",
			"@loglayer/transport",
			"@loglayer/transport-simple-pretty-terminal",
			"@wordpress/api-fetch",
			"@wordpress/components",
			"@wordpress/data",
			"@wordpress/dataviews",
			"@wordpress/element",
			"@wordpress/hooks",
			"@wordpress/i18n",
			"@wordpress/interactivity",
			"@wpkernel/core",
			"@wpkernel/ui",
			"loglayer",
			"react",
			"react-dom",
			"react/jsx-dev-runtime",
			"react/jsx-runtime"
		]
	},
	"assetManifest": {
		"path": "build/index.asset.json"
	}
}
