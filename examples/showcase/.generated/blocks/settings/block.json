{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "acme-jobs/settings",
	"title": "Settings",
	"description": "Settings block generated from project config",
	"category": "widgets",
	"icon": "database",
	"textdomain": "acme-jobs",
	"keywords": ["settings"],
	"supports": {
		"align": ["wide", "full"],
		"color": {
			"background": true,
			"text": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		}
	},
	"editorScriptModule": "file:./index.tsx",
	"viewScriptModule": "file:./view.ts",
	"attributes": {
		"retention_days": {
			"type": "integer"
		},
		"notify_email": {
			"type": "string"
		},
		"auto_archive": {
			"type": "boolean"
		}
	}
}
