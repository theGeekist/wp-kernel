{
	"core": "WordPress/WordPress#6.7",
	"phpVersion": "8.3",
	"plugins": [
		"./examples/showcase-plugin"
	],
	"themes": [
		"https://downloads.wordpress.org/theme/twentytwentyfour.1.3.zip"
	],
	"port": 8888,
	"testsPort": 8889,
	"env": {
		"development": {
			"port": 8888,
			"config": {
				"WP_DEBUG": true,
				"WP_DEBUG_LOG": true,
				"WP_DEBUG_DISPLAY": true,
				"SCRIPT_DEBUG": true,
				"WP_ENVIRONMENT_TYPE": "local"
			}
		},
		"tests": {
			"port": 8889,
			"config": {
				"WP_DEBUG": true,
				"WP_DEBUG_LOG": true,
				"WP_DEBUG_DISPLAY": false,
				"SCRIPT_DEBUG": false,
				"WP_ENVIRONMENT_TYPE": "local"
			}
		}
	},
	"mappings": {
		"wp-content/plugins/wp-kernel": "./packages/kernel",
		"wp-content/plugins/wp-kernel-ui": "./packages/ui"
	},
	"lifecycleScripts": {
		"afterStart": "bash ./test-harness/wp-env/seeds/init-if-needed.sh"
	}
}
