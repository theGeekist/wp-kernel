{
	"$schema": "https://playground.wordpress.net/blueprint-schema.json",
	"landingPage": "/",
	"preferredVersions": {
		"php": "8.1",
		"wp": "6.7.4"
	},
	"features": {
		"networking": true
	},
	"steps": [
		{
			"step": "login"
		},
		{
			"step": "defineWpConfigConsts",
			"consts": {
				"WP_DEBUG": true,
				"WP_DEBUG_LOG": true,
				"WP_DEBUG_DISPLAY": false
			}
		},
		{
			"step": "setSiteOptions",
			"options": {
				"permalink_structure": "/%postname%/",
				"users_can_register": 0
			}
		}
	]
}
