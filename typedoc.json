{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": [
		"packages/core/src/index.ts",
		"packages/ui/src/index.ts",
		"packages/cli/src/index.ts",
		"packages/test-utils/src/index.ts",
		"packages/php-json-ast/src/index.ts"
	],
	"entryPointStrategy": "resolve",
	"tsconfig": "tsconfig.docs.json",
	"out": "docs/api/generated",
	"plugin": ["typedoc-plugin-markdown"],
	"readme": "none",
	"skipErrorChecking": true,
	"excludePrivate": true,
	"excludeProtected": true,
	"excludeInternal": true,
	"excludeExternals": true,
	"hideGenerator": true,
	"gitRevision": "main",
	"name": "WP Kernel API",
	"includeVersion": true,
	"disableSources": true,
	"sort": ["kind", "alphabetical"],
	"kindSortOrder": [
		"Function",
		"Class",
		"Interface",
		"TypeAlias",
		"Variable",
		"Enum"
	],
	"categorizeByGroup": true,
	"categoryOrder": [
		"Metadata",
		"HTTP",
		"Resource",
		"Actions",
		"Capability",
		"Data",
		"Interactivity",
		"Events",
		"Reporter",
		"Namespace",
		"Pipeline",
		"Error",
		"Jobs",
		"Policy",
		"*"
	],
	"validation": {
		"notExported": true,
		"invalidLink": true,
		"notDocumented": false
	},
	"fileExtension": ".md",
	"flattenOutputFiles": false,
	"useCodeBlocks": true
}
