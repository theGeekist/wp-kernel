{
	"name": "@wpkernel/ui",
	"version": "0.4.0",
	"description": "UI component library for WP Kernel",
	"license": "EUPL-1.2",
	"type": "module",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/theGeekist/wp-kernel.git",
		"directory": "packages/ui"
	},
	"homepage": "https://github.com/theGeekist/wp-kernel#readme",
	"bugs": {
		"url": "https://github.com/theGeekist/wp-kernel/issues"
	},
	"keywords": [
		"wordpress",
		"components",
		"ui",
		"gutenberg",
		"blocks",
		"react"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./hooks/useAction": {
			"types": "./dist/hooks/useAction.d.ts",
			"import": "./dist/hooks/useAction.js",
			"default": "./dist/hooks/useAction.js"
		},
		"./hooks/usePolicy": {
			"types": "./dist/hooks/usePolicy.d.ts",
			"import": "./dist/hooks/usePolicy.js",
			"default": "./dist/hooks/usePolicy.js"
		},
		"./hooks/resource-hooks": {
			"types": "./dist/hooks/resource-hooks.d.ts",
			"import": "./dist/hooks/resource-hooks.js",
			"default": "./dist/hooks/resource-hooks.js"
		},
		"./hooks/usePrefetcher": {
			"types": "./dist/hooks/usePrefetcher.d.ts",
			"import": "./dist/hooks/usePrefetcher.js",
			"default": "./dist/hooks/usePrefetcher.js"
		},
		"./hooks/useHoverPrefetch": {
			"types": "./dist/hooks/useHoverPrefetch.d.ts",
			"import": "./dist/hooks/useHoverPrefetch.js",
			"default": "./dist/hooks/useHoverPrefetch.js"
		},
		"./hooks/useVisiblePrefetch": {
			"types": "./dist/hooks/useVisiblePrefetch.d.ts",
			"import": "./dist/hooks/useVisiblePrefetch.js",
			"default": "./dist/hooks/useVisiblePrefetch.js"
		},
		"./hooks/useNextPagePrefetch": {
			"types": "./dist/hooks/useNextPagePrefetch.d.ts",
			"import": "./dist/hooks/useNextPagePrefetch.js",
			"default": "./dist/hooks/useNextPagePrefetch.js"
		},
		"./dataviews": {
			"types": "./dist/dataviews/index.d.ts",
			"import": "./dist/dataviews/index.js",
			"default": "./dist/dataviews/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build && tsc --noEmit",
		"clean": "rm -rf dist *.tsbuildinfo",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
		"format:check": "prettier --check 'src/**/*.{js,ts,tsx,json,md}'",
		"typecheck": "tsc --noEmit",
		"typecheck:tests": "tsc --project tsconfig.tests.json --noEmit",
		"test": "jest --passWithNoTests --watchman=false",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage --watchman=false"
	},
	"peerDependencies": {
		"@wpkernel/core": "workspace:*",
		"@testing-library/react": ">=16.3.0",
		"@wordpress/dataviews": ">=9.1.0",
		"@wordpress/components": ">=28.0.0",
		"@wordpress/data": ">=10.0.0",
		"@wordpress/element": ">=6.0.0",
		"react": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
