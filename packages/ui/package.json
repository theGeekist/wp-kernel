{
	"name": "@geekist/wp-kernel-ui",
	"version": "0.1.1",
	"description": "UI component library for WP Kernel",
	"license": "EUPL-1.2",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/theGeekist/wp-kernel.git",
		"directory": "packages/ui"
	},
	"homepage": "https://github.com/theGeekist/wp-kernel#readme",
	"bugs": {
		"url": "https://github.com/theGeekist/wp-kernel/issues"
	},
	"keywords": [
		"wordpress",
		"components",
		"ui",
		"gutenberg",
		"blocks",
		"react"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build && tsc --noEmit",
		"clean": "rm -rf dist *.tsbuildinfo",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
		"format:check": "prettier --check 'src/**/*.{js,ts,tsx,json,md}'",
		"typecheck": "tsc --noEmit",
		"typecheck:tests": "tsc --project tsconfig.tests.json --noEmit",
		"test": "jest --passWithNoTests",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"peerDependencies": {
		"@geekist/wp-kernel": "workspace:*",
		"@wordpress/components": ">=28.0.0",
		"@wordpress/data": ">=10.0.0",
		"@wordpress/element": ">=6.0.0",
		"react": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
