{
	"name": "@geekist/wp-kernel-ui",
	"version": "0.0.0",
	"description": "UI component library for WP Kernel",
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/theGeekist/wp-kernel.git",
		"directory": "packages/ui"
	},
	"homepage": "https://github.com/theGeekist/wp-kernel#readme",
	"bugs": {
		"url": "https://github.com/theGeekist/wp-kernel/issues"
	},
	"keywords": [
		"wordpress",
		"components",
		"ui",
		"gutenberg",
		"blocks",
		"react"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "tsc --build --watch",
		"build": "tsc --build",
		"clean": "rm -rf dist *.tsbuildinfo",
		"typecheck": "tsc --noEmit"
	},
	"peerDependencies": {
		"@geekist/wp-kernel": "workspace:*",
		"@wordpress/components": ">=28.0.0",
		"@wordpress/element": ">=6.0.0",
		"react": ">=18.0.0"
	},
	"devDependencies": {
		"@types/react": "^19.0.8",
		"typescript": "^5.9.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
