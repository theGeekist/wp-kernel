[
  "final class JobCategoriesController extends BaseController",
  "{",
  "        public function get_resource_name(): string",
  "        {",
  "                return 'jobCategories';",
  "        }",
  "",
  "        public function get_schema_key(): string",
  "        {",
  "                return 'jobCategory';",
  "        }",
  "",
  "        public function get_rest_args(): array",
  "        {",
  "                return [];",
  "        }",
  "",
  "        /**",
  "         * Handle [GET] /kernel/v1/job-categories.",
  "         * @wp-kernel route-kind list",
  "         */",
  "        public function getKernelV1JobCategories( WP_REST_Request $request )",
  "        {",
  "                $taxonomy = $this->getJobCategoriesTaxonomy();",
  "                $per_page = (int) $request->get_param('per_page');",
  "                if ($per_page <= 0) {",
  "                        $per_page = 10;",
  "                }",
  "                if ($per_page > 100) {",
  "                        $per_page = 100;",
  "                }",
  "",
  "                $page = (int) $request->get_param('page');",
  "                if ($page <= 0) {",
  "                        $page = 1;",
  "                }",
  "",
  "                $query_args = [",
  "                        'taxonomy' => $taxonomy,",
  "                        'hide_empty' => false,",
  "                ];",
  "",
  "                $extra_args = $request->get_params();",
  "                foreach ( $extra_args as $key => $value ) {",
  "                        if (in_array($key, [ 'page', 'per_page', 'taxonomy', 'hide_empty' ], true)) {",
  "                                continue;",
  "                        }",
  "",
  "                        $query_args[$key] = $value;",
  "                }",
  "",
  "                $query_args['number'] = $per_page;",
  "                $query_args['offset'] = ($page - 1) * $per_page;",
  "",
  "                $term_query = new WP_Term_Query();",
  "                $results = $term_query->query($query_args);",
  "                if (is_wp_error($results)) {",
  "                        return $results;",
  "                }",
  "",
  "                $items = [];",
  "                foreach ( $results as $term ) {",
  "                        if ($term instanceof WP_Term) {",
  "                                $items[] = $this->prepareJobCategoriesTermResponse($term);",
  "                        }",
  "                }",
  "",
  "                $count_query_args = $query_args;",
  "                $count_query_args['count'] = true;",
  "                $count_query_args['number'] = 0;",
  "                $count_query_args['offset'] = 0;",
  "",
  "                $count_query = new WP_Term_Query();",
  "                $total = (int) $count_query->query($count_query_args);",
  "                $pages = (int) ceil($total / max(1, $per_page));",
  "",
  "                return [",
  "                        'items' => $items,",
  "                        'total' => $total,",
  "                        'pages' => $pages,",
  "                ];",
  "        }",
  "",
  "        /**",
  "         * Handle [GET] /kernel/v1/job-categories/:slug.",
  "         * @wp-kernel route-kind get",
  "         */",
  "        public function getKernelV1JobCategoriesSlug( WP_REST_Request $request )",
  "        {",
  "                $slug = $request->get_param( 'slug' );",
  "",
  "                $identity = $this->validateJobCategoriesIdentity($request->get_param('slug'));",
  "                if (is_wp_error($identity)) {",
  "                        return $identity;",
  "                }",
  "",
  "                $term = $this->resolveJobCategoriesTerm($identity);",
  "                if (!$term instanceof WP_Term) {",
  "                        return new WP_Error('wpk_job_categories_not_found', 'Unable to locate JobCategories term.', [ 'status' => 404 ]);",
  "                }",
  "",
  "                return $this->prepareJobCategoriesTermResponse($term);",
  "        }",
  "",
  "        private function getJobCategoriesTaxonomy(): string",
  "        {",
  "                return 'job_category';",
  "        }",
  "",
  "        private function prepareJobCategoriesTermResponse( WP_Term $term ): array",
  "        {",
  "                return [",
  "                        'id' => (int) $term->term_id,",
  "                        'slug' => (string) $term->slug,",
  "                        'name' => (string) $term->name,",
  "                        'taxonomy' => (string) $term->taxonomy,",
  "                        'hierarchical' => true,",
  "                        'description' => (string) $term->description,",
  "                        'parent' => (int) $term->parent,",
  "                        'count' => (int) $term->count,",
  "                ];",
  "        }",
  "",
  "        private function resolveJobCategoriesTerm( $identity ): ?WP_Term",
  "        {",
  "                $taxonomy = $this->getJobCategoriesTaxonomy();",
  "                if (is_int($identity)) {",
  "                        $term = get_term($identity, $taxonomy);",
  "                        if ($term instanceof WP_Term) {",
  "                                return $term;",
  "                        }",
  "                }",
  "                if (is_string($identity)) {",
  "                        $candidate = trim(strval($identity));",
  "                        if ('' !== $candidate) {",
  "                                $term = get_term_by('slug', $candidate, $taxonomy);",
  "                                if ($term instanceof WP_Term) {",
  "                                        return $term;",
  "                                }",
  "",
  "                                $term = get_term_by('name', $candidate, $taxonomy);",
  "                                if ($term instanceof WP_Term) {",
  "                                        return $term;",
  "                                }",
  "                        }",
  "                }",
  "                return null;",
  "        }",
  "",
  "        private function validateJobCategoriesIdentity( $value )",
  "        {",
  "                if (null === $value) {",
  "                        return new WP_Error('wpk_job_categories_missing_identifier', 'Missing identifier for JobCategories.', [ 'status' => 400 ]);",
