{
	"name": "@wpkernel/php-json-ast",
	"version": "0.8.0",
	"description": "Utilities and type definitions for working with PHP-Parser JSON AST output",
	"license": "EUPL-1.2",
	"type": "module",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/theGeekist/wp-kernel.git",
		"directory": "packages/php-json-ast"
	},
	"homepage": "https://github.com/theGeekist/wp-kernel#readme",
	"bugs": {
		"url": "https://github.com/theGeekist/wp-kernel/issues"
	},
	"keywords": [
		"wordpress",
		"php",
		"ast",
		"json",
		"parser"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./builders": {
			"types": "./dist/builders.d.ts",
			"import": "./dist/builders.js",
			"default": "./dist/builders.js"
		},
		"./channels": {
			"types": "./dist/channels.d.ts",
			"import": "./dist/channels.js",
			"default": "./dist/channels.js"
		},
		"./nodes": {
			"types": "./dist/nodes.d.ts",
			"import": "./dist/nodes.js",
			"default": "./dist/nodes.js"
		},
		"./factories": {
			"types": "./dist/factories.d.ts",
			"import": "./dist/factories.js",
			"default": "./dist/factories.js"
		},
		"./factories/*": {
			"types": "./dist/factories/*.d.ts",
			"import": "./dist/factories/*.js",
			"default": "./dist/factories/*.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"import": "./dist/types.js",
			"default": "./dist/types.js"
		},
		"./modifiers": {
			"types": "./dist/modifiers.d.ts",
			"import": "./dist/modifiers.js",
			"default": "./dist/modifiers.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build && tsc --noEmit",
		"clean": "rm -rf dist dist-tests *.tsbuildinfo",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
		"format:check": "prettier --check 'src/**/*.{js,ts,tsx,json,md}'",
		"typecheck": "tsc --noEmit",
		"typecheck:tests": "tsc --project tsconfig.tests.typecheck.json --noEmit && tsc --project tsconfig.tests.json --noEmit",
		"test": "jest --passWithNoTests --watchman=false",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage --watchman=false"
	},
	"publishConfig": {
		"access": "public"
	},
	"peerDependencies": {
		"@wpkernel/core": "workspace:*",
		"@wpkernel/test-utils": "workspace:*"
	},
	"dependencies": {
		"@wpkernel/php-driver": "workspace:*"
	}
}
