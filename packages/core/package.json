{
	"name": "@wpkernel/core",
	"version": "0.4.0",
	"description": "Rails-like framework for building modern WordPress products",
	"license": "EUPL-1.2",
	"type": "module",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/theGeekist/wp-kernel.git",
		"directory": "packages/core"
	},
	"homepage": "https://github.com/theGeekist/wp-kernel#readme",
	"bugs": {
		"url": "https://github.com/theGeekist/wp-kernel/issues"
	},
	"keywords": [
		"wordpress",
		"framework",
		"blocks",
		"gutenberg",
		"interactivity",
		"actions",
		"resources",
		"rest-api"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./http": {
			"types": "./dist/http/index.d.ts",
			"import": "./dist/http.js",
			"default": "./dist/http.js"
		},
		"./resource": {
			"types": "./dist/resource/index.d.ts",
			"import": "./dist/resource.js",
			"default": "./dist/resource.js"
		},
		"./error": {
			"types": "./dist/error/index.d.ts",
			"import": "./dist/error.js",
			"default": "./dist/error.js"
		},
		"./actions": {
			"types": "./dist/actions/index.d.ts",
			"import": "./dist/actions.js",
			"default": "./dist/actions.js"
		},
		"./data": {
			"types": "./dist/data/index.d.ts",
			"import": "./dist/data.js",
			"default": "./dist/data.js"
		},
		"./events": {
			"types": "./dist/events/index.d.ts",
			"import": "./dist/events/index.js",
			"default": "./dist/events/index.js"
		},
		"./policy": {
			"types": "./dist/policy/index.d.ts",
			"import": "./dist/policy.js",
			"default": "./dist/policy.js"
		},
		"./reporter": {
			"types": "./dist/reporter/index.d.ts",
			"import": "./dist/reporter.js",
			"default": "./dist/reporter.js"
		},
		"./namespace": {
			"types": "./dist/namespace/index.d.ts",
			"import": "./dist/namespace/index.js",
			"default": "./dist/namespace/index.js"
		},
		"./namespace/constants": {
			"types": "./dist/namespace/constants.d.ts",
			"import": "./dist/namespace/constants.js",
			"default": "./dist/namespace/constants.js"
		},
		"./contracts": {
			"types": "./dist/contracts/index.d.ts",
			"import": "./dist/contracts.js",
			"default": "./dist/contracts.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "vite build --watch",
		"build": "vite build && tsc --noEmit",
		"clean": "rm -rf dist *.tsbuildinfo",
		"lint": "eslint src/",
		"lint:fix": "eslint src/ --fix",
		"format": "prettier --write 'src/**/*.{js,ts,tsx,json,md}'",
		"format:check": "prettier --check 'src/**/*.{js,ts,tsx,json,md}'",
		"typecheck": "tsc --noEmit",
		"typecheck:tests": "tsc --project tsconfig.tests.json --noEmit",
		"test": "jest --passWithNoTests",
		"test:ci": "jest --passWithNoTests --watchman=false",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"peerDependencies": {
		"@wordpress/api-fetch": ">=7.0.0",
		"@wordpress/data": ">=10.0.0",
		"@wordpress/element": ">=6.0.0",
		"@wordpress/hooks": ">=4.0.0",
		"loglayer": ">=6.7.2",
		"@loglayer/shared": ">=2.4.0",
		"@loglayer/transport": ">=2.3.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {}
}
